; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[ {
"base": "Node",
"class": "Section",
"language": "GDScript",
"path": "res://scripty/controls/Suggestion/Section.gd"
} ]
_global_script_class_icons={
"Section": ""
}

[application]

config/name="Project teal"
config/description="A testing of a submarine tactics game"
run/main_scene="res://Scenes/MainScene.tscn"
boot_splash/image="res://assets/testing/ico.png"
boot_splash/bg_color=Color( 0, 0, 0, 1 )
config/icon="res://assets/testing/icomini.png"
config/windows_native_icon="res://assets/1661910279.ico"

[autoload]

network="res://scripty/networking/network.gd"
gamestate="res://scripty/networking/gamestate.gd"

[display]

window/size/width=1920
window/size/height=1080
window/size/resizable=false
window/size/fullscreen=true
window/size/test_width=1280
window/size/test_height=720
window/stretch/mode="viewport"
window/stretch/aspect="expand"

